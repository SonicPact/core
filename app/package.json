{
  "name": "sonicpact",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbopack",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "typegen:db": "cd supabase && supabase gen types typescript --project-id miijjpygwmedrzwqbfow > ../src/database.generated.ts",
    "db:pull": "cd supabase && supabase link --project-ref miijjpygwmedrzwqbfow && supabase db pull",
    "db:push": "cd supabase && supabase link --project-ref miijjpygwmedrzwqbfow && supabase db push --include-all",
    "update-idl": "cd .. && npm run update-idl",
    "sync-idl": "cd .. && anchor build && npm run update-idl",
    "generate-nft-metadata": "cd .. && npm run generate-nft-metadata",
    "upload-nft-metadata": "cd .. && npm run upload-nft-metadata",
    "init-dev": "cd .. && npm run init-dev"
  },
  "dependencies": {
    "@noble/ed25519": "^2.0.0",
    "@radix-ui/react-slot": "^1.1.2",
    "@solana/wallet-adapter-base": "^0.9.23",
    "@solana/wallet-adapter-react": "^0.15.35",
    "@solana/wallet-adapter-react-ui": "^0.9.35",
    "@solana/wallet-adapter-wallets": "^0.19.32",
    "@solana/web3.js": "^1.89.1",
    "@supabase/ssr": "^0.6.1",
    "@supabase/supabase-js": "^2.49.1",
    "bs58": "^5.0.0",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "lucide-react": "^0.482.0",
    "next": "15.2.2",
    "pino-pretty": "^13.0.0",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "supabase": "^2.19.7",
    "tailwind-merge": "^3.0.2",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@tailwindcss/postcss": "^4",
    "@types/bs58": "^4.0.4",
    "@types/node": "^22.13.10",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@types/uuid": "^9.0.8",
    "eslint": "^9",
    "eslint-config-next": "15.2.2",
